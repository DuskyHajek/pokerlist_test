{
  "framework": "vite",
  "rewrites": [
    {
      "source": "/pokerlist-api-detail",
      "destination": "https://pokerlist.com/pl/pokerclub.php"
    },
    {
      "source": "/pokerlist-api",
      "destination": "https://pokerlist.com/pl/pokerclubs.php"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.7.3"
    }
  }
} 